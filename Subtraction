//===============================
//	Subtraction Program
//===============================

	public class DiffOfNumbers  
	{  
		public static void main(String args[])   
		{  
			int n1 = 225, n2 = 115, diff;  
			diff = n1 - n2;  
			System.out.println("The diff of numbers is: "+diff);  
		}  
	}  
